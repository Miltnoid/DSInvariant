let arith : exp -> nat =
  let rec f1 (e1:exp) : nat =
    match e1 with
      | Const n1 -> n1
      | Sum (e2, e3) -> sum (f1 e3) (f1 e2)
      | Prod (e2, e3) -> mult (f1 e3) (f1 e2)
      | Pred e2 -> (match f1 e2 with
                      | O -> O
                      | S n1 -> n1)
      | Max (e2, e3) -> (match compare (f1 e3) (f1 e2) with
                           | LT -> f1 e2
                           | EQ -> f1 e2
                           | GT -> f1 e3)
  in
    f1
