let desugar : src -> dst =
  let rec f1 (s1:src) : dst =
    match s1 with
      | NumS n1 -> NumD n1
      | PlusS (s2, s3) -> BinopD (PlusOp, f1 s2, f1 s3)
      | MinusS (s2, s3) -> BinopD (MinusOp, f1 s2, f1 s3)
      | TrueS -> BoolD True
      | FalseS -> BoolD False
  in
    f1
