let zip : list -> list -> pairlist_option =
  let rec f1 (l1:list) : list -> pairlist_option =
    fun (l2:list) ->
      match l1 with
        | Nil -> (match l2 with
                    | Nil -> Some PNil
                    | Cons (b1, l3) -> None)
        | Cons (b1, l3) -> (match l2 with
                              | Nil -> None
                              | Cons (b2, l4) -> (match f1 l3 l4 with
                                                    | None -> None
                                                    | Some p1 -> Some (
                                                                   PCons 
                                                                    ((b1, b2),
                                                                    p1))))
  in
    f1
